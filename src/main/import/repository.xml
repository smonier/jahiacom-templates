<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <jahiacom-templates j:dependencies="animate bootstrap4-components bootstrap4-core default facets font-awesome jexperience jexperience-components jquery popperjs search"
                          j:installedModules="jahiacom-templates"
                          j:modulePriority="0"
                          j:moduleType="templatesSet"
                          j:title="Jahiacom Templates"
                          jcr:mixinTypes="jmix:hasExternalProviderExtension"
                          jcr:primaryType="jnt:module">
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder"/>
         <templates jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder">
               <themes jcr:primaryType="jnt:folder">
                  <default jcr:primaryType="jnt:folder"/>
               </themes>
            </files>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <base j:theme="#/modules/jahiacom-templates/templates/files/themes/default"
                  j:view="jahiacom-templates"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
               <home jcr:primaryType="jnt:pageTemplate"/>
               <partner j:applyOn="jcnt:partner"
                        j:defaultTemplate="true"
                        j:hiddenTemplate="false"
                        jcr:primaryType="jnt:contentTemplate">
                  <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Partner"/>
                  <pagecontent jcr:primaryType="jnt:contentList">
                     <header j:allowedTypes="jcnt:hero" jcr:primaryType="jnt:area"/>
                     <main-resource-display j:mainResourceView="detail" jcr:primaryType="jnt:mainResourceDisplay"/>
                     <more jcr:primaryType="jnt:area"/>
                  </pagecontent>
               </partner>
               <customer j:applyOn="jcnt:blogEntry jcnt:customer"
                         j:defaultTemplate="true"
                         j:hiddenTemplate="false"
                         jcr:primaryType="jnt:contentTemplate">
                  <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Blog"/>
                  <pagecontent jcr:primaryType="jnt:contentList">
                     <header j:allowedTypes="jcmix:banner" jcr:primaryType="jnt:area"/>
                     <main-resource-display j:mainResourceView="detail" jcr:primaryType="jnt:mainResourceDisplay"/>
                     <more jcr:primaryType="jnt:area"/>
                  </pagecontent>
               </customer>
               <resource j:applyOn="jcnt:resource"
                         j:defaultTemplate="true"
                         j:hiddenTemplate="false"
                         jcr:primaryType="jnt:contentTemplate">
                  <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Partner"/>
                  <pagecontent jcr:primaryType="jnt:contentList">
                     <header j:allowedTypes="jcmix:banner" jcr:primaryType="jnt:area"/>
                     <main-resource-display j:mainResourceView="detail" jcr:primaryType="jnt:mainResourceDisplay"/>
                     <more jcr:primaryType="jnt:area"/>
                  </pagecontent>
               </resource>
               <_x0032_-column j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                  <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="2 column"/>
                  <pagecontent jcr:primaryType="jnt:contentList">
                     <page-top grid="12"
                               jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:predefinedGrid"
                               jcr:primaryType="bootstrap4nt:grid"
                               rowHorizontalAlignment="default"
                               rowVerticalAlignment="default"
                               typeOfGrid="predefinedGrid"/>
                     <page-main grid="8_4"
                                gridLayout="fixed-width"
                                jcr:mixinTypes="bootstrap4mix:createContainer bootstrap4mix:createRow bootstrap4mix:createSection bootstrap4mix:predefinedGrid"
                                jcr:primaryType="bootstrap4nt:grid"
                                rowHorizontalAlignment="default"
                                rowVerticalAlignment="default"
                                sectionCssClass="bg-white"
                                sectionElement="section"
                                typeOfGrid="predefinedGrid"/>
                     <page-bottom grid="12"
                                  jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:predefinedGrid"
                                  jcr:primaryType="bootstrap4nt:grid"
                                  rowHorizontalAlignment="default"
                                  rowVerticalAlignment="default"
                                  typeOfGrid="predefinedGrid"/>
                  </pagecontent>
               </_x0032_-column>
               <default j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                  <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="default"/>
                  <pagecontent jcr:primaryType="jnt:contentList">
                     <page-top grid="12"
                               jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:predefinedGrid"
                               jcr:primaryType="bootstrap4nt:grid"
                               rowHorizontalAlignment="default"
                               rowVerticalAlignment="default"
                               typeOfGrid="predefinedGrid"/>
                     <page-main grid="12"
                                jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:predefinedGrid"
                                jcr:primaryType="bootstrap4nt:grid"
                                rowHorizontalAlignment="default"
                                rowVerticalAlignment="default"
                                typeOfGrid="predefinedGrid"/>
                     <page-bottom grid="12"
                                  jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:predefinedGrid"
                                  jcr:primaryType="bootstrap4nt:grid"
                                  rowHorizontalAlignment="default"
                                  rowVerticalAlignment="default"
                                  typeOfGrid="predefinedGrid"/>
                  </pagecontent>
               </default>
            </base>
            <content-template j:applyOn="jnt:content"
                              j:hiddenTemplate="true"
                              j:priority="1"
                              j:view="jahiacom-templates"
                              jcr:primaryType="jnt:contentTemplate">
               <pagecontent jcr:primaryType="jnt:contentList">
                  <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
               </pagecontent>
            </content-template>
         </templates>
         <home j:isHomePage="true"
               j:templateName="home"
               jcr:primaryType="jnt:page">
            <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
         </home>
      </jahiacom-templates>
   </modules>
</content>